{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "linux": "\uf17c",
            "macos": "\uf179",
            "ubuntu": "\uf31b",
            "windows": "\ue62a"
          },
          "template": " {{.Icon}} {{if .WSL}} (WSL){{end}}",
          "foreground": "#61AFEF",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf2c0 {{.HostName}}\ue0bb{{.UserName}}\u2800",
          "foreground": "#F3C267",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "Bash",
              "cmd": "Cmd",
              "powershell": "Shell",
              "pwsh": "Shell"
            }
          },
          "template": " \ue795 {{ .Name }}",
          "foreground": "#E06C75",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#F3C267",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#83c092{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#FF9248{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " x{{ reason .Code }}",
          "foreground": "#b3ffde",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "roundrock"
          },
          "template": " {{ .FormattedMs }} · ",
          "foreground": "#b3ffde",
          "type": "executiontime",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\uf00d",
            "charging_icon": "\uf0e7",
            "color_background": true,
            "discharging_icon": "\uf242"
          },
          "template": "{{ if eq .State.String \"Charging\" }}\uf0e7{{ end }}{{ if ge .Percentage 90 }}\udb80\udc82{{ else if ge .Percentage 80 }}\udb80\udc81{{ else if ge .Percentage 70 }}\udb80\udc80{{ else if ge .Percentage 60 }}\udb80\udc7f{{ else if ge .Percentage 50 }}\udb80\udc7e{{ else if ge .Percentage 40 }}\udb80\udc7d{{ else if ge .Percentage 30 }}\udb80\udc7c{{ else if ge .Percentage 20 }}\udb80\udc7b{{ else if ge .Percentage 10 }}\udb80\udc7a{{ else if ge .Percentage 0 }}\udb80\udc8e{{ end }} {{.Percentage}}% · ",
          "foreground": "#b3ffde",
          "background": "transparent",
          "type": "battery",
          "style": "plain",
          "foreground_templates": [
            "{{ if lt .Percentage 20 }}#FF0000{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "Monday<#fff>,</> 02/01/06 <#fff>|</> 15:04"
          },
          "template": "[{{ .CurrentDate | date .Format }}]",
          "foreground": "#E5C07B",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_template": "<#fff> » </>",
            "home_icon": "\ueb06",
            "style": "full"
          },
          "template": " \ue5ff  [ {{ .Path }} ]",
          "foreground": "#61AFEF",
          "type": "path",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "!",
          "foreground": "#E06C75",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " ❯",
          "foreground": "#83c092",
          "background": "transparent",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{if gt .Code 0}}#E06C75{{end}}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
