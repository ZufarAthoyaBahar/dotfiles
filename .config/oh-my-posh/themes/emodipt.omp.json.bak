{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#61AFEF",
          "properties": {
            "windows": "\ue62a",
            "linux": "\uf17c",
            "ubuntu": "\uf31b",
            "macos": "\uf179"
          },
          "style": "plain",
          "template": " {{.Icon}} {{if .WSL}} (WSL){{end}}",
          "type": "os"
        },
        {
          "foreground": "#F3C267",
          "style": "plain",
          "template": " \uf2c0 {{.HostName}}\ue0bb{{.UserName}}\u2800",
          "type": "session"
        },
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#E06C75",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "Shell",
              "powershell": "Shell",
              "cmd": "Cmd",
              "bash": "Bash"
            }
          },
          "template": " \ue795 {{ .Name }}"
        },
        {
          "foreground": "#F3C267",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#83c092{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#FF9248{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#b3ffde",
          "foreground_templates": ["{{ if gt .Code 0 }}#E06C75{{ end }}"],
          "template": " x{{ reason .Code }}"
        },
        {
          "foreground": "#b3ffde",
          "foreground_templates": ["{{ if gt .Code 0 }}#E06C75{{ end }}"],
          "properties": {
            "style": "roundrock",
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} · ",
          "type": "executiontime"
        },
        {
          "type": "battery",
          "style": "plain",
          "background": "transparent",
          "foreground": "#b3ffde",
          "template": "{{ if eq .State.String \"Charging\" }}\uf0e7{{ end }}{{ if ge .Percentage 90 }}\udb80\udc82{{ else if ge .Percentage 80 }}\udb80\udc81{{ else if ge .Percentage 70 }}\udb80\udc80{{ else if ge .Percentage 60 }}\udb80\udc7f{{ else if ge .Percentage 50 }}\udb80\udc7e{{ else if ge .Percentage 40 }}\udb80\udc7d{{ else if ge .Percentage 30 }}\udb80\udc7c{{ else if ge .Percentage 20 }}\udb80\udc7b{{ else if ge .Percentage 10 }}\udb80\udc7a{{ else if ge .Percentage 0 }}\udb80\udc8e{{ end }} {{.Percentage}}% · ",
          "properties": {
            "charged_icon": "\uf00d",
            "charging_icon": "\uf0e7",
            "discharging_icon": "\uf242",
            "color_background": true
          },
          "foreground_templates": ["{{ if lt .Percentage 20 }}#FF0000{{ end }}"]
        },
        {
          "foreground": "#E5C07B",
          "properties": {
            "time_format": "Monday<#fff>,</> 02/01/06 <#fff>|</> 15:04"
          },
          "style": "plain",
          "template": "[{{ .CurrentDate | date .Format }}]",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#61AFEF",
          "properties": {
            "style": "full",
            "folder_separator_template": "<#fff> \u00bb </>",
            "home_icon": "\ueb06"
          },
          "style": "plain",
          "template": " \ue5ff  [ {{ .Path }} ]",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "\u0021",
          "type": "root"
        },
        {
          "background": "transparent",
          "foreground": "#83c092",
          "foreground_templates": ["{{if gt .Code 0}}#E06C75{{end}}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \u276f",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
